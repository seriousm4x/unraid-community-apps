<?xml version="1.0"?>
<Container version="2">
  <Name>UpSnap</Name>
  <Repository>ghcr.io/seriousm4x/upsnap:4</Repository>
  <Registry>https://github.com/seriousm4x/UpSnap/pkgs/container/upsnap</Registry>
  <Network>host</Network>
  <MyIP />
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/137262-support-seriousm4x-upsnap/</Support>
  <Project>https://github.com/seriousm4x/UpSnap</Project>
  <Overview>A simple wake on lan web app written with SvelteKit, Go and PocketBase.</Overview>
  <Category>HomeAutomation: Network:Management Status:Stable</Category>
  <WebUI>http://[IP]:[PORT:8090]</WebUI>
  <TemplateURL>
    https://raw.githubusercontent.com/seriousm4x/unraid-community-apps/master/templates/upsnap.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/seriousm4x/unraid-community-apps/master/icons/upsnap.png</Icon>
  <ExtraParams />
  <PostArgs>--http 0.0.0.0:8090</PostArgs>
  <CPUset />
  <DateInstalled>1680434971</DateInstalled>
  <DonateText>Want to support my work? Buy me a coffee on ko-fi or become a Github Sponsor.</DonateText>
  <DonateLink>https://github.com/seriousm4x#want-to-buy-me-a--</DonateLink>
  <Requires />
  <Config Name="pb_data" Target="/app/pb_data/" Default="" Mode="rw" Description="upsnap database"
    Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/upsnap/</Config>
  <Config Name="Timezone" Target="TZ" Default="Europe/London" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"/>
</Container>
